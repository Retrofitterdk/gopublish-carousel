{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "squareonesoftware/demo-carousel",
  "version": "0.1.0",
  "title": "Carousel",
  "category": "widgets",
  "icon": "media-interactive",
  "description": "A carousel block using the Interactivity API.",
  "example": {},
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "interactivity": true
  },
  "attributes": {
    "columns": {
      "type": "number",
      "default": 3
    },
    "autoplay": {
      "type": "boolean",
      "default": false
    },
    "scroll": {
      "type": "number",
      "default": 1
    }
  },
  "textdomain": "demo-carousel",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScriptModule": "file:./view.js"
}